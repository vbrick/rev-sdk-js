{
  "name": "@vbrick/rev-sdk",
  "version": "0.1.0",
  "description": "Client SDK for Vbrick Rev",
  "author": "Vbrick Systems",
  "repository": "github:vbrick/rev-sdk-js",
  "type": "module",
  "module": "dist/rev-sdk.esm.js",
  "browser": "dist/rev-sdk.js",
  "types": "dist/index.d.ts",
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "scripts": {
